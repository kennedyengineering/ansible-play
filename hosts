# need to specify ansible_user for ansible to not try logging in as different user, important for the difference in the bootstrapping routing and the actual install routine

[local]
localhost

[testpi]
testing_raspberrypi 	ansible_user=pi

[remote_bootstrap]
jetson_nano		ansible_user=dev
kali_raspberrypi	ansible_user=root
testing_raspberrypi	ansible_user=pi

[remote]
jetson_nano		ansible_user=ansible	
kali_raspberrypi	ansible_user=ansible
testing_raspberrypi	ansible_user=ansible
